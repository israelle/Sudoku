<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dadan.application_sudoku">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FenetreLevel1"
            android:label="@string/title_activity_fenetre_level1"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FenetreLevel2"
            android:label="@string/title_activity_fenetre_level2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FenetreLevel3"
            android:label="@string/title_activity_fenetre_level3"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Sudoku_grille"
            android:label="@string/title_activity_sudoku_grille"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>